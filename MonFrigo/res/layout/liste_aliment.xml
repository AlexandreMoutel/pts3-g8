<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/nomAliment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="16dp"
        android:textSize="18sp"
        android:text="TextView" />

    <TextView
        android:id="@+id/quantiteAliment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/dateAliment"
        android:layout_alignParentRight="true"
        android:layout_marginRight="36dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/typeAliment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/nomAliment"
        android:layout_below="@+id/nomAliment"
        android:text="TextView" />

    <TextView
        android:id="@+id/dateAliment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/typeAliment"
        android:layout_below="@+id/typeAliment"
        android:text="TextView" />
    
    <ListView
        android:id="@+id/android:list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />
     
</RelativeLayout>